Proceso tarea3
	Definir codigo Como Caracter;
	definir cont como entero;
	cont = 0;
	Repetir
		Escribir 'indica tu contraseña';
		Leer codigo;
        Escribir "contraseña incorrecta";
		cont = cont + 1;
	Hasta Que codigo='a1234' o cont = 3
	si codigo = "a1234" Entonces
		Escribir 'bienvenido';
	sino 
		Escribir "superaste la cantidad maxima de errores";
	FinSi
FinProceso
